{
  "name": "techplatform-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http://localhost:7980",
  "resolutions": {
    "styled-components": "^5"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix --ext js,jsx,ts,tsx src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts --coverage --ui",
    "postinstall": "patch-package",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn lint",
      "yarn prettier:fix",
      "yarn prettier"
    ]
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
